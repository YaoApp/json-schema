        },
        "id": {
          "type": "string",
          "description": "Unique Identifier"
        },
        "name": {
          "type": "string",
          "description": "Form Name, Supports Multiple Languages"
        },
        "action": {
          "$ref": "#/definitions/YaoForm.ActionDSL",
          "description": "Form Data Interaction. Used to specify the handler for data reading, saving, etc., set data hooks, bind models, etc."
        },
        "layout": {
          "$ref": "#/definitions/YaoForm.LayoutDSL",
          "description": "Form Interface Layout"
        },
        "fields": {
