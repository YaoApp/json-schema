        "guard": {
          "type": "string"
        },
        "bind": {
          "$ref": "#/definitions/YaoForm.BindActionDSL",
          "description": "Bind model or form. Set the associated handler and default presentation for the form based on the Widget ID."
        },
        "setting": {
          "$ref": "#/definitions/YaoAction.Process",
          "description": "Associated Handler. Returns form page configuration"
        },
        "component": {
          "$ref": "#/definitions/YaoAction.Process"
        },
        "upload": {
          "$ref": "#/definitions/YaoAction.Process"
        },
        "download": {
          "$ref": "#/definitions/YaoAction.Process"
        },
        "find": {
          "$ref": "#/definitions/YaoAction.Process",
          "description": "Associated Handler. Specifies the handler for querying a single data by primary key and default parameters, returning a single record."
        },
        "save": {
          "$ref": "#/definitions/YaoAction.Process",
          "description": "Associated Handler. Specifies the handler for saving a single data"
        },
        "update": {
          "$ref": "#/definitions/YaoAction.Process",
          "description": "Associated Handler. Specifies the handler for updating a single data by primary key"
        },
        "create": {
          "$ref": "#/definitions/YaoAction.Process",
          "description": "Associated Handler. Specifies the handler for creating a single data"
        },
        "delete": {
          "$ref": "#/definitions/YaoAction.Process",
          "description": "Associated Handler. Specifies the handler for deleting a single data by primary key"
        },
        "before:find": {
          "$ref": "#/definitions/YaoHook.Before",
          "description": "Before Hook. Runs before the associated handler, inputting the user's parameter table and returning the processed parameter table"
        },
        "after:find": {
          "$ref": "#/definitions/YaoHook.After",
          "description": "Runs after the associated handler find"
        },
        "before:save": {
          "$ref": "#/definitions/YaoHook.Before",
          "description": "Before Hook. Runs before the associated handler, inputting the user's parameter table and returning the processed parameter table"
        },
        "after:save": {
          "$ref": "#/definitions/YaoHook.After",
          "description": "Runs after the associated handler save"
        },
        "before:create": {
          "$ref": "#/definitions/YaoHook.Before",
          "description": "Before Hook. Runs before the associated handler, inputting the user's parameter table and returning the processed parameter table"
        },
        "after:create": {
          "$ref": "#/definitions/YaoHook.After",
          "description": "Runs after the associated handler create"
        },
        "before:delete": {
          "$ref": "#/definitions/YaoHook.Before",
          "description": "Before Hook. Runs before the associated handler, inputting the user's parameter table and returning the processed parameter table"
        },
        "after:delete": {
          "$ref": "#/definitions/YaoHook.After",
          "description": "Runs after the associated handler delete"
        },
        "before:update": {
          "$ref": "#/definitions/YaoHook.Before",
          "description": "Runs after the associated handler update"
        },
        "after:update": {
          "$ref": "#/definitions/YaoHook.After",
          "description": "After Hook. Runs before the associated handler, inputting the result returned by the associated handler, returning the processed result"
        }
