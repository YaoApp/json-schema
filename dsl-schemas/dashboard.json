    "YaoComponent.Compute": {
      "type": "object",
      "properties": {
        "process": {
          "type": "string",
          "description": "If the compute type equals string, it will automatically convert to a processor name and pass four parameters to the processor, with the first being the value of the field."
        },
        "args": {
          "anyOf": [
            {
              "type": "array",
              "items": {
                "$ref": "#/definitions/YaoComponent.CArg"
              }
            },
            {
              "type": "array",
              "items": {
                "type": "string"
              }
            }
          ],
          "description": "Use expressions, referencing the structure of input data. Default parameter list: [\"$C(value)\", \"$C(props)\", \"$C(type)\", \"$C(id)\"]"
        }
      },
      "required": [
        "process"
      ],
      "additionalProperties": false,
      "description": "Data value calculation, Parameter list: $C(row) Current row data, $C(value) Current row current field value, $C(props) Current component props, $C(type) Current component type, $C(id) Current Widget ID 'hello' String constant 1024 Integer constant 0.618 Float constant"
    },
    "YaoComponent.CArg": {
      "type": "object",
      "properties": {
        "isExp": {
          "type": "boolean",
          "description": "Is it an expression? If yes, key will be used to bind to the structure."
        },
        "key": {
          "type": "string",
          "description": "Effective when isExp=true, use key to reference field names in the model structure"
        },
        "value": {
          "description": "Effective when isExp=false, can be any object. If it's a string, :: can be used as a prefix to format the output"
        }
      },
      "required": [
        "isExp",
        "key",
        "value"
      ],
      "additionalProperties": false,
      "description": "Custom parameter table for compute processors"
    },
